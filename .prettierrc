{
  "printWidth": 140,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": ["*.component.html"],
      "options": {
        "parser": "angular"
      }
    },
    {
      "files": ["*.html"],
      "options": {
        "parser": "html"
      }
    }
  ]
}
