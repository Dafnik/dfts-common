{
  "name": "ng-icons-manager",
  "version": "1.1.3",
  "description": "CLI utility for Angular projects that automatically scans your templates for used icons from @ng-icons packages and copies only those into your project folder.",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/Dafnik/dfts-common/tree/main/libs/ng-icons-manager",
  "sideEffects": true,
  "keywords": [
    "ng",
    "ngx",
    "angular2",
    "angular",
    "typescript",
    "icons",
    "ng-icons"
  ],
  "author": {
    "name": "Dafnik",
    "email": "contact@dafnik.me",
    "url": "https://dafnik.me"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/Dafnik"
    },
    {
      "type": "liberapay",
      "url": "https://liberapay.com/Dafnik"
    }
  ],
  "bugs": {
    "url": "https://github.com/Dafnik/dfts-common/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Dafnik/dfts-common.git",
    "directory": "libs/ng-icons-manager"
  },
  "bin": {
    "ng-icons-manager": "./cli.cjs.js"
  },
  "dependencies": {
    "tslib": "^2"
  },
  "peerDependencies": {
    "chokidar": "5",
    "glob": "13"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "main": "./index.cjs.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs.js"
      }
    }
  }
}
