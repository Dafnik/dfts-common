{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "ng-icons-manager",
  "projectType": "library",
  "sourceRoot": "libs/ng-icons-manager",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "options": {
        "additionalEntryPoints": ["libs/ng-icons-manager/bin/cli.js"],
        "assets": ["libs/ng-icons-manager/README.md", "libs/ng-icons-manager/CHANGELOG.md", "libs/ng-icons-manager/LICENSE"],
        "format": ["cjs"],
        "main": "libs/ng-icons-manager/src/index.ts",
        "outputPath": "dist/libs/ng-icons-manager",
        "tsConfig": "libs/ng-icons-manager/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      },
      "options": {
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "changelogHeader": " ",
        "push": false
      }
    }
  }
}
